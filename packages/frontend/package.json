{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "npm-run-all build:**",
    "build:clean": "rm -rf ../backend/public/**",
    "build:app": "react-scripts build",
    "build:move-to-server": "mv ./build/* ../backend/public",
    "build:rename-index": "mv ../backend/public/index.html ../backend/public/_index.html",
    "build-canary": "npm-run-all build-canary:**",
    "build-canary:clean": "rm -rf ../backend/canary/**",
    "build-canary:app": "react-scripts build",
    "build-canary:move-to-server": "mv ./build/* ../backend/canary",
    "build-canary:rename-index": "mv ../backend/canary/index.html ../backend/canary/_index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
